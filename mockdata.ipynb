    "\n",
    "letters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ'\n",
    "diagnostic_codes = [f\"{random.choice(letters)}{random.randint(100, 999)}\" for _ in range(10)]\n",
    "\n",
    "\n",
    "# Example: Mock-up of generating random dates\n",
    "def generate_random_dates(num_dates):\n",
    "    #base_date = datetime(2022, 1, 1)\n",
    "    return [datetime(random.randint(2006, 2022), 1, 1) + timedelta(days=random.randint(0, 365)) for _ in range(num_dates)]\n",
    "\n",
    "# Example: Generating admission and discharge dates with discharge being later than admission\n",
    "admission_dates = generate_random_dates(num_entries)\n",
    "discharge_dates = [ad_date + timedelta(days=random.randint(1, 30)) for ad_date in admission_dates]"
   ]
  },
