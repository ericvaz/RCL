   "source": [
    "\n",
    "# Generate the DataFrame\n",
    "df_mock = pd.DataFrame({\n",
    "    'rec_id': range(1000000, 1000000 + num_entries),\n",
    "    'SUBMITTING_PROV_CODE': [random.choice([5, 6, 7, 8, 9]) for _ in range(num_entries)],\n",
    "    'FISCAL_YEAR': [(admission_dates[_] - pd.DateOffset(months=3)).year for _ in range(num_entries)],\n", #fiscal year is offset by 3 months
    "    'urban_rural_remote': [random.choice(['Urban', 'Rural/Remote']) for _ in range(num_entries)],\n",
    "    'FSA': [random.choice(fsa_codes) for _ in range(num_entries)],\n",
    "    'QAIPPE': [random.randint(1, 5) for _ in range(num_entries)],\n",
    "    'GENDER_CODE': [random.choice(['M', 'M', 'F', 'F', 'U']) for _ in range(num_entries)],\n",
    "    'age_group': [random.choice(['19-21', '22-24', '25-27', '28-30','31-33', '34-36', '37-39', '40-42', '43-45', '46-48', '49-51', '52-54', '55-57', '58-61', '62-64', '65+']) for _ in range(num_entries)], #removed under 18 for consumption\n",
    "    'ADMIT_BY_AMBULANCE_IND': [random.choice(['G', 'N']) for _ in range(num_entries)],\n",
    "    'ADMISSION_DATE': [admission_dates[_].strftime('%d-%b-%y') for _ in range(num_entries)],\n",
    "    'DISCHARGE_DATE': [discharge_dates[_].strftime('%d-%b-%y') for _ in range(num_entries)],\n",
    "    'DISCH_DISPOSITION_GROUP': [random.choice(['HOME', 'HOME', 'TRANSFER', 'TRANSFER', 'DEATH']) for _ in range(num_entries)],\n",
    "    'BLOOD_TRANSFUSION_IND': [random.choice(['Y', 'N']) for _ in range(num_entries)],\n",
    "    'DIAG_CODE_1': [random.choice(diagnostic_codes) for _ in range(num_entries)],\n",
    "    'DIAG_CODE_2': [random.choice(diagnostic_codes) for _ in range(num_entries)],\n",
    "    'DIAG_CODE_3': [random.choice(diagnostic_codes) for _ in range(num_entries)],\n",
    "    'DIAG_CODE_4': [random.choice(diagnostic_codes) for _ in range(num_entries)],\n",
    "    'GLASGOW_COMA_SCALE': [random.choice(range(3, 16)) for _ in range(num_entries)]\n",
    "    'SEATBELT_INDICATOR': [random.choice(['Y', 'N', ""]) for _ in range(num_entries)],
    "    'HELMET_INDICATOR': [random.choice(['Y', 'N', ""]) for _ in range(num_entries)]})
    "})\n",
